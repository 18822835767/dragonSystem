<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeTableView?>

<!--外邦人打开评价页面时会加载该View-->
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="controller.MyEvaluationController" prefHeight="400.0" prefWidth="600.0">
    <HBox >
        <Button text="修改评价"  prefWidth="80" onAction="#changeMyEvaluation"/>
        <Button text="删除评价"  prefWidth="80" onAction="#deleteMyEvaluation"/>
    </HBox>

    <TreeTableView fx:id="treeTableView"/>
</VBox>
